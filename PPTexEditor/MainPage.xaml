<Page
    x:Class="PPTexEditor.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PPTexEditor"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <RichEditBox x:Name="textBox" Margin="10,84,10,10" TextWrapping="Wrap" IsSpellCheckEnabled="True" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition" AcceptsReturn="True"/>
        <Button x:Name="button" Content="Compile" VerticalAlignment="Top" Margin="10,10,0,0" Width="89"/>
        <Button x:Name="button_Copy" Content="New Measurement" VerticalAlignment="Top" Margin="104,10,0,0" Click="button_Copy_Click"/>
        <Button x:Name="button_Copy1" Content="Insert Equation" VerticalAlignment="Top" Margin="104,47,0,0" Width="145" Click="button_Copy1_Click"/>
        <Button x:Name="button_Copy2" Content="New Table" VerticalAlignment="Top" Margin="10,47,0,0" d:LayoutOverrides="HorizontalAlignment"/>
        <Button x:Name="button_Copy3" Content="Start" VerticalAlignment="Top" Margin="255,10,0,0" Click="button_Copy3_Click"/>

    </Grid>
</Page>
